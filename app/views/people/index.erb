<h1>People</h1>
<table>
  <thread>
    <th>Name</th>
    <th>Birthdate</th>
  </thread>
  <tbody>
    <% @people.each do |person| %>
    <tr>
      <td>
        <a href="<%= "/people/#{person.id}" %>">
          <%= "#{person.first_name} #{person.last_name}" %>
        </a>
      </td>
      <td>
        <%= "#{person.birthdate}" %>
      </td>
    </tr>
  <% end %>
</tbody>
</table>
